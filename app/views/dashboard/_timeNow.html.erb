<div class="mdl-grid" style="padding: 0;">
  <div class="mdl-cell mdl-cell--12-col mdl-cell--12-col-tablet mdl-cell--12-col-phone">
    <div class="mdl-card mdl-shadow--2dp next-card general-cards-1">
      <div class="flex-row" style="padding: 10px">
        <div class="flex-col-xs-7" style="margin-left: 10px; margin-top: 35px;">
          <h3 class="mdl-card__title-text" style="font-size: 80px;"><%= t('hello') %></h3>
          <br>
          <span class="hello-user"><%= "#{current_account.first_name} #{current_account.last_name}" %></span>
        </div>
        <div class="flex-col-xs right-align flex-start">
          <div class="print-time-now">
            <div id="get-time-now" style="font-size: 48px; font-weight: 100;"></div>
            <p><span style="font-size: 10px"> <%= l Date.today, :format => :format_nextgad %></span></p>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
