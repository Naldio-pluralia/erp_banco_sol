<%= simple_form_for(assessment_setting) do |f| %>
  <%= form_url_helpers(f)%>
  <div id="assessment_setting-modal-form" class="-modal -modal-fixed-footer">
    <div class="-modal-content">
      <span class="h5-size"> <%= assessment_setting.new_record? ? t('.new') : t('.edit') %> </span>
      <hr>

      <div class="row">
      </div>
    </div>

    <div class="modal-footer">
      <%= f.button :submit %>
      <%= link_to t('cancel'), assessment_setting.cancel_url || assessment_settings_path, class: 'waves-effect waves-light btn btn-flat btn-cancel-form' %>
    </div>
  </div>
<% end %>
